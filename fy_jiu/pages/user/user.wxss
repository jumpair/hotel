.shade {
  width: 100%;
  height: 100%;
  position: fixed;
  background: #333;
  z-index: 77;
  top: 0;
  left: 0;
  opacity: 0.8;
}

.room {
  left: 50%;
  top: 45%;
  transform: translate(-50%,-50%);
  position: fixed;
  z-index: 99;
  background: white;
  border-radius: 12rpx;
  overflow: hidden;
}

.room_div {
  position: relative;
}

.close {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.close image {
  width: 30rpx;
  height: 30rpx;
}

.room_content_div {
  text-align: center;
  margin: 0rpx auto;
}

.room_content_div text {
  font-size: 16px;
  color: #099fde;
  margin-top: 15rpx;
}

.orderCode {
  max-width: 90vw;
  height: 60vh;
  margin-bottom: 15rpx;
}

.savebutton {
  color: white;
