/* 首页热门城市 */

.hot_city{
  padding: 30rpx 30rpx 0rpx;
}
.hot_city .list_{
  margin: -5rpx;
}
.hot_city .list_:after{
  content: '';
  display: block;
  clear: both;
}
.hot_city .hot_city_item{
  float: left;
  width: 25%;
  height: 120rpx;
  position: relative;
  padding: 5rpx;
  box-sizing: border-box;
}
.hot_city text{
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 10;
  font-weight: bold;
  color: white;
  font-size: 32rpx;
  transform: translate(-50%, -50%);

}
.hot_city image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
