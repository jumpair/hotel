/* 改版酒店css代码 */
.dlayout{
 position: absolute;
  width: 100%;
  height: 450rpx;
  background: white;
  top:390rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;

}

.dlayout2{
  width: 93%;
  margin: 0 auto;

}

.daddress{
  width: 100%;
  height: 160rpx;
  font-size: 14px;
  color: #808080;
  display: flex;
  justify-content: space-between
}
.address_left{
  width: 100%;
  word-break: break-all;
  margin-top: 45rpx;
}
.address_right image{
  display: block;
  margin-top: 35rpx;
    width: 69px;
    height: 46px;
}
.dserice{
  height: 70rpx;
  width: 100%;
  border-top:1px solid #eee;
}
.dserice{
  padding-top:25rpx; 
  color: #808080;
  display: flex;
  justify-content: space-between;
}
.dserice view{
  display:flex;
}
.dserice image{
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin: 5rpx 10rpx 0 0 ;
}

.dview{
  margin: 30rpx 30rpx 0;
  height: 140rpx;
  background: var(--main-color);
  border-radius: 12rpx;
  box-shadow: 0px 8px 8px 0px rgba(176,176,176,0.15);
  color: #ffffff;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  padding: 36rpx 36rpx 28rpx;
  box-sizing: border-box;
}
.dview .left__{
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
}
.dview .left__ .h_{
  width: 70rpx;
  height: 40rpx;
  position: relative;
}
.dview .left__ .h_:after{
  content: '';
  width: 20rpx;
  height: 0px;
  opacity: 1;
  border: 2rpx solid #F5F5F5;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.dview_item>view:nth-child(1){
  font-size: 20rpx;
  font-weight: 400;
  color: #EEEEEE;
}
.item_date{
  margin-top: 1rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
}
.dview .left__ .img{
  width: 32rpx;
  height: 40rpx;
  margin-left: 10rpx;
}
.dview .left__ .img image{
  width: 32rpx;
  height: 32rpx;
}
.dview .cont{
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
