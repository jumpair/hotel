}
.score_info_img image{
  display: inline-block;
  width: 22rpx;
  height: 22rpx;
}
.score_info_txt view{
  display: inline-block;
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
}
.score_info_txt text{
  display: inline-block;
  font-size: 24rpx;
  font-weight: 400;
  color: #333333;
}
.score_lx{
  width: 139rpx;
