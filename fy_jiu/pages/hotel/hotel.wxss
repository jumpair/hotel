

page{
  padding-bottom: 120rpx;
  width: 100%;
  box-sizing: border-box;
  overflow-x: hidden;
  background-color: #F9F9F9;
}

.index_top{
  background-color: transparent;
}
.index_top .fixed_{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  z-index: 999;
}
.index_top .fixed_ image{
  width: 20rpx;
  height: 40rpx;
  position: absolute;
  left: 0rpx;
  top: 50%;
  transform: translateY(-50%);
}

.hotel_pic{
  position: relative;
}
.back_page{
  width: 74rpx;
  line-height: 66rpx;
  position: fixed;
  top: 66rpx;
  left: 0;
  z-index: 999;
}
.back_page image{
  width: 20rpx;
  height: 36rpx;
  margin: 0 auto;
}
.hotel_pic>image{
  width: 100%;
  height: 460rpx;
}
.hotel_pic_num{
  width: 110rpx;
  line-height: 40rpx;
  background: rgba(0,0,0,.5);
  text-align: center;
  border-radius: 60rpx;
  position: absolute;
  top: 350rpx;
  right: 30rpx;
}
.hotel_pic_num image{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  margin: 0 8rpx 0 0;
  position: relative;
  top: 4rpx;
}
.hotel_pic_num text{
  display: inline-block;
  font-size: 22rpx;
  line-height: 40rpx;
  color: #fff;
}
.hotel_all{
  border-radius: 12rpx;
  padding-bottom: 30rpx;
  background-color: #fff;
  position: relative;
  top: -20rpx;
}
.hotel_info{
  padding: 30rpx 0 28rpx 32rpx;
  display: flex;
}
.hotel_info_txt{
  flex: 1;
}
.hotel_info_txt_tit{
  font-size: 32rpx;
  line-height: 84rpx;
  color: #101010;
}
.hotel_info_txt_tab{
  overflow: hidden;
}
.hotel_info_txt_tab view{
  float: left;
  font-size: 18rpx;
  line-height: 32rpx;
  color: var(--main-color);
  border-radius: 10rpx;
  background-color: #F5F0E6;
  padding: 0 10rpx;
  margin-right: 8rpx;
}
.hotel_intr{
  float: right;
  border-radius: 60rpx 0 0 60rpx;
  font-size: 20rpx;
  line-height: 60rpx;
  width: 144rpx;
  height: 60rpx;
