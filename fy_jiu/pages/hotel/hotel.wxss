  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding-left: 38rpx;
}
.hotel_intr text{
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  padding-left: 20rpx;
}
.hotel_intr image{
  width: 32rpx;
  height: 32rpx;
}
.score{
  padding-left: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.score .up_{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.score_num{
  font-size: 46rpx;
  width: 120rpx;
