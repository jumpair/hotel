page{
  background: #f5f5f5;
}
.bottom{
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  bottom:0px;
}

.address_div{
  width: 100%;
  height: 130rpx;
  display: flex;
  background: #fff;
}
.address_info{
  width: 88%;
  padding-top: 20rpx;
  font-size: 13px;
  color: #666;
  padding-left: 20rpx;
  
}
.address_ico{
  width: 12%;
  height: 130rpx;
  line-height: 130rpx;
  text-align: center;
  margin-top:10rpx;
}
.address_ico image{
  width: 40rpx;
  height: 40rpx;
}
.address_info_top{
  display: flex;
  flex-direction: row;
justify-content: space-between;
margin-bottom: 5rpx;
}
.address_line{
  width: 100%;
  height: 7px;
}
.address_line image{
  width: 100%;
  height: 7px;
}
.address_edit{
  width: 100%;
  height: 70rpx;
  border-top:1px solid #eee;
  display: flex;
  line-height: 70rpx;
   color: #666;
}
.address_edit image{
    width: 33rpx;
    height: 33rpx;
}
.address_edit_view{
  display: flex;
  margin: 0 20rpx;
}
.address_edit_img{
  margin-top:8rpx;
  margin-right:5rpx;
}


