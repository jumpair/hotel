}
.room_div .w_w view{
  font-size: 24rpx;
  font-weight: 400;
  color: #333333;
}
.room_div .w_w text{
  font-size: 24rpx;
  font-weight: 400;
  color: var(--main-color);
}
.room_div .w_w:not(:first-child){
  margin-top: 6rpx;
}
.room_div .price_item{
  padding: 30rpx 0;
  border-bottom: 1rpx solid #dddddd;
}
.room_div .day view{
  color: #999999;
}
.room_div .day text{
  color: #999999;
}







