.list_title{
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
  line-height: 48rpx;
  padding-bottom:30rpx;
}

.PlaceOrder_list_all{
  background-color: #fff;
  margin-top: 32rpx;
  padding: 26rpx 30rpx;
  box-shadow: var(--box-shadow);
  
}
.PlaceOrder_list{
  border-bottom: 1rpx solid #eeeeee;
  display: flex;
  position: relative;
}
.PlaceOrder_list_name{
  font-size: 26rpx;
  line-height: 94rpx;
  color: #272727;
  width: 170rpx;
}
.select_person{
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 2;
}
.select_person image{
  width: 20rpx;
  height: 20rpx;
}
.select_person view{
  height: 40rpx;
  width: 40rpx;
  border: 1rpx solid #dddddd;
  display: flex;
  align-items: center;
  justify-content: center;
}
.select_person view:nth-child(2){
  width: 60rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #333333;
  text-align: center;
  border-left-color: transparent;
  border-right-color: transparent;
}
.select_person view:nth-child(3){
  background-color: var(--main-color);
}
.person_list{
  margin-top: 32rpx;
}
.person_item{
  background: #F9F9F9;
  border-radius: 12rpx;
  border: 2rpx solid #EEEEEE;
  padding: 32rpx 28rpx;
  margin-bottom: 32rpx;
}
.person_item:last-child{
  margin-bottom: 10rpx;
}
.person_item .up__{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.person_item .up__ view{
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}
.person_item .up__ text{
  font-size: 28rpx;
  font-weight: bold;
  color: #BE9C58;
}



.PlaceOrder_list_sel{
  flex: 1;
  font-size: 26rpx;
  line-height: 94rpx;
  color: #272727;
}

.PlaceOrder_list input{
  flex: 1;
  font-size: 26rpx;
  line-height: 94rpx;
  height: 94rpx;
  color: #272727;
}
