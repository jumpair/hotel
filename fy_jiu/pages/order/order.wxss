/* pages/PlaceOrder/PlaceOrder.wxss */
page{
  padding-bottom: 108rpx;
  background-color: #F9F9F9;
}
.PlaceOrder_info{
  background-color: #ffffff;
  padding: 30rpx;
}
.PlaceOrder_info_con{
  border-radius: 10rpx;
  background-color: #fff;
  padding: 28rpx 34rpx;
}
.PlaceOrder_info_tit view{
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  line-height: 32rpx;
}
.PlaceOrder_info_tit text{
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 24rpx;
}
.time__{
  height: 140rpx;
  background: #F5F0E6;
  border-radius: 12rpx;
  opacity: 1;
  margin-top: 20rpx;
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  padding: 0 34rpx 28rpx;
  box-sizing: border-box;
}
.time__ .l_{
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
}
.time__ .l_ .item_time view{
  font-size: 20rpx;
  font-weight: 400;
  color: #999999;
  line-height: 30rpx;
}
.time__ .l_ .item_time text{
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  line-height: 42rpx;
}
.time__ .l_ .h_{
  width: 80rpx;
  height: 48rpx;
  position: relative;
}
.time__ .l_ .h_:after{
  content: '';
  width: 20rpx;
  height: 0px;
  opacity: 1;
  border: 2rpx solid #333333;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.time__ .r_{
  font-size: 24rpx;
  font-weight: 400;
  color: #000000;
  line-height: 48rpx;
}

.PlaceOrder_info_txt{
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 36rpx;
  padding-top: 22rpx;
}
.PlaceOrder_info_txt text{
  margin: 0 15rpx;
}
